data Tree a where
	Empty :: Tree a
	Node :: x: a -> l: Tree a -> r: Tree a -> Tree a
  
termination measure size :: Tree a -> {Int | _v >= 0} where
  Empty -> 0
  Node x l r -> size l + size r + 1
  
zero :: {Int | _v == 0}
one :: {Int | _v == 1}
plus :: x: Int -> y: Int -> {Int | _v == x + y}
	
countNodes :: t: Tree a -> {Int | _v == size t}
