all :: <p :: a -> Bool> . arg1:(x:a -> {Bool|_v == p x}) -> arg0:List {a|p _v} -> {Bool|_v}
all = \f . \xs . 
    match xs with
      Nil -> True
      Cons y ys -> and (f y) (all f
                                ys)

allPos :: {Bool|_v}
allPos = all (\x . x > 0) (Cons
                             1 (Cons 2 (Cons 3 Nil)))

