incr :: n:Int -> f:(m:Int -> {Int|_v == n}) -> {Int|_v == n + 1}
incr = \n . \f . 1 + (f 0)

f :: {Int|_v == 2}
f = incr (incr zero (\x7 .
              zero)) (\x . 1)

