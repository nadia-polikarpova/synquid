qualifier {x <= 0, x >= 0, x != 0}

type Nat = {Int | _v >= 0}

measure len :: List a -> {Int | _v >= 0}

data List a decreases len where
	Nil :: {List a | len _v == 0}
	Cons :: x: a -> xs: List a -> {List a | len _v == len xs + 1}

neg :: x: Int -> {Int | _v == -x}	
map :: (a -> b) -> xs: List a -> {List b | len _v == len xs}	
	
toNat :: xs: List Int -> {List Nat | len _v == len xs}
